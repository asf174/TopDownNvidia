all: tags
	/usr/bin/python3 setup.py build

dist:
	/usr/bin/python3 setup.py sdist

install: tags
	/usr/bin/python3 setup.py install

clean:
	rm -Rf build
	rm -f tags

tags:
	ctags -R src || true

tests:
	echo "running tests"
	find ./test -type f -name "*.py" -exec python {} \;
