
#!/bin/bash




for i in $(seq 1 1 10); do
    (time topdown.py -f ./srad 2048 2048 0 "127 0 127 0.5 2" -l3 -nd) >> time_topdown.txt 2>&1
    (time ./srad 2048 2048 0 127 0 127 0.5 2) >> time_native.txt 2>&1
done
topdown.py -f srad 2048 2048 0 "127 0 127 0.5 2" -l3 -nd -os scan_file.txt -o results_topdown.txt
